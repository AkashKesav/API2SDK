openapi: 3.0.0
info:
  title: Click API
  description: >-
    Click is a lead management and sales automation platform that helps
    businesses streamline their sales processes and improve customer engagement.
    This API provides programmatic access to Click's features, allowing
    developers to integrate Click's functionality into their own applications
    and workflows.
  version: 1.0.0
servers:
  - url: http://localhost:8080
components:
  securitySchemes:
    bearerAuth:
      type: http
      scheme: bearer
security:
  - bearerAuth: []
tags:
  - name: Service Versions
  - name: Auth
paths:
  /service-versions:
    get:
      tags:
        - Service Versions
      summary: Get Service Versions
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /auth/config:
    get:
      tags:
        - Auth
      summary: Get Auth Config
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /.well-known/openid-configuration:
    get:
      tags:
        - Auth
      summary: Get Well Known Oidc Configuration
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /.well-known/jwks.json:
    get:
      tags:
        - Auth
      summary: Get Well Known Oidc Json Web Key Set
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
